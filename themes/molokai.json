{
  "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
  "name": "Molokai",
  "author": "Enes Gen√ß <enecgenc@gmail.com>",
"themes": [
        {
            "name": "Molokai Dark",
            "appearance": "dark",
            "style": {
                "background": "#1B1D1E",
                "text": "#F8F8F2",
                "text.muted": "#7E8E91",
                "text.placeholder": "#465457",
                "text.disabled": "#808080",
                "text.accent": "#F92672",

                "border": "#465457",
                "border.variant": "#808080",
                "border.focused": "#FD971F",
                "border.selected": "#F92672",

                "surface.background": "#232526",
                "elevated_surface.background": "#293739",

                "element.background": "#232526",
                "element.hover": "#293739",
                "element.active": "#3E3D32",
                "element.selected": "#403D3D",
                "element.disabled": "#1E0010",

                "ghost_element.background": "transparent",
                "ghost_element.hover": "#293739",
                "ghost_element.active": "#3E3D32",
                "ghost_element.selected": "#403D3D",

                "editor.background": "#1B1D1E",
                "editor.foreground": "#F8F8F2",
                "editor.line_number": "#465457",
                "editor.active_line_number": "#FD971F",
                "editor.active_line.background": "#293739",
                "editor.indent_guide": "#3B3A32",
                "editor.active_indent_guide": "#666666",

                "status_bar.background": "#1B1D1E",
                "tab.active_background": "#1B1D1E",
                "tab.inactive_background": "#232526",
                "tab_bar.background": "#1B1D1E",

                "terminal.background": "#1B1D1E",
                "terminal.foreground": "#F8F8F2",
                "terminal.bright_foreground": "#FFFFFF",
                "terminal.dim_foreground": "#7E8E91",

                "terminal.ansi.black": "#1B1D1E",
                "terminal.ansi.bright_black": "#465457",
                "terminal.ansi.red": "#F92672",
                "terminal.ansi.bright_red": "#F92672",
                "terminal.ansi.green": "#A6E22E",
                "terminal.ansi.bright_green": "#A6E22E",
                "terminal.ansi.yellow": "#E6DB74",
                "terminal.ansi.bright_yellow": "#E6DB74",
                "terminal.ansi.blue": "#66D9EF",
                "terminal.ansi.bright_blue": "#66D9EF",
                "terminal.ansi.magenta": "#AE81FF",
                "terminal.ansi.bright_magenta": "#AE81FF",
                "terminal.ansi.cyan": "#66D9EF",
                "terminal.ansi.bright_cyan": "#66D9EF",
                "terminal.ansi.white": "#F8F8F2",
                "terminal.ansi.bright_white": "#FFFFFF",

                "syntax": {
                    "keyword": {
                        "color": "#F92672",
                        "font_weight": 700
                    },
                    "function": {
                        "color": "#A6E22E"
                    },
                    "comment": {
                        "color": "#7E8E91",
                        "font_style": "italic"
                    },
                    "string": {
                        "color": "#E6DB74"
                    },
                    "number": {
                        "color": "#AE81FF"
                    },
                    "constant": {
                        "color": "#AE81FF",
                        "font_weight": 700
                    },
                    "type": {
                        "color": "#66D9EF"
                    },
                    "identifier": {
                        "color": "#FD971F"
                    },
                    "operator": {
                        "color": "#F92672"
                    },
                    "punctuation": {
                        "color": "#F8F8F2"
                    }
                },

                "error": "#F92672",
                "error.background": "#1E0010",
                "warning": "#FD971F",
                "warning.background": "#4C4745",
                "info": "#66D9EF",
                "info.background": "#232526",
                "hint": "#A6E22E",
                "hint.background": "#3E3D32"
            }
        }
    ]
}
